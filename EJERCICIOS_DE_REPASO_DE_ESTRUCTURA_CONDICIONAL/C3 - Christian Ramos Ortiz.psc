Algoritmo Diagrama
	Definir n1,n2 como real;
	Escribir "[ DIVISIÓN ]";
	Escribir "Introduce el primer número:";
	Leer n1;
	Escribir "Introduce el segundo número:";
	Leer n2;
	Escribir "--------------------------------";
	Escribir "";
	
	Si n2!=0 Entonces
		Escribir "El resultado de la división es igual a: ",n1/n2;
	SiNo
		Escribir "El segundo número no puede ser cero";
	Fin Si
	
FinAlgoritmo
